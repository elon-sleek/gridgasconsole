# Supabase (NEXT_PUBLIC_ prefix required for client-side access)
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=

# Server-only (for Next.js Route Handlers / API routes)
# Never prefix with NEXT_PUBLIC_ and never expose to the browser.
SUPABASE_SERVICE_ROLE_KEY=

# ThingsBoard (if the portal reads telemetry directly)
NEXT_PUBLIC_TB_BASE_URL=

# Maps
NEXT_PUBLIC_MAP_API_KEY=

# Admin portal secrets
# Server-only PIN gate (optional). If unset, PIN gate is disabled.
ADMIN_PIN=
